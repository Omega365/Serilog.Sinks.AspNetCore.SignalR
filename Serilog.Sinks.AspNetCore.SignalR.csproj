<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <PackageVersion>0.3.5</PackageVersion>
        <PackageId>Serilog.Sinks.AspNetCore.SignalR</PackageId>
        <Title>Serilog.Sinks.AspNetCore.SignalR</Title>
        <Authors>Maximilian St√ºmpfl</Authors>
        <Description>This is a Serilog Sink for Asp.net core 3, to send logs over SignalR</Description>
        <RepositoryUrl>https://github.com/lTimeless/Serilog.Sinks.AspNetCore.SignalR</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Serilog, SignalR, Sinks</PackageTags>
        <PackageIconUrl>https://serilog.net/images/serilog-sink-nuget.png</PackageIconUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.SignalR" Version="1.1.0" />
      <PackageReference Include="Serilog" Version="2.9.0" />
    </ItemGroup>

</Project>
